<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns="" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" id="request">
	<xs:element name="request" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="head">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="transcode" type="xs:string"/>
							<xs:element name="reqtime" type="xs:string"/>
							<xs:element name="systype" type="xs:string"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="body">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="membid" type="xs:string"/>
							<xs:element name="listofcontact">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="contact">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="listofpersonaladdress">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="personaladdress" minOccurs="0" maxOccurs="unbounded">
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="addrpriflag" type="xs:string"/>
																			<xs:element name="addrtype" type="xs:string"/>
																			<xs:element name="addr" type="xs:string"/>
																			<xs:element name="continent" type="xs:string"/>
																			<xs:element name="state" type="xs:string"/>
																			<xs:element name="province" type="xs:string"/>
																			<xs:element name="city" type="xs:string"/>
																			<xs:element name="area" type="xs:string"/>
																			<xs:element name="street" type="xs:string"/>
																			<xs:element name="postcode" type="xs:string"/>
																			<xs:element name="updt" type="xs:string"/>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="response" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="head">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="transcode" type="xs:string"/>
							<xs:element name="resptime" type="xs:string"/>
							<xs:element name="systype" type="xs:string"/>
							<xs:element name="retcode" type="xs:string"/>
							<xs:element name="retmsg" type="xs:string"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="body">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="membid" type="xs:string"/>
							<xs:element name="recode" type="xs:string"/>
							<xs:element name="remsg" type="xs:string"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
